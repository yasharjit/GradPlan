        />

        <script type="text/javascript">
            WebFont.load({
                google: {
                    families: ["Sen:regular,700,800"],
                },
            });
        </script>
        <link
            href="public/images/favicon.png"
            rel="icon"
            type="image/gif"
            sizes="16x16"
        />
