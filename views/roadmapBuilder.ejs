                height: 320px;
                border-radius: 50%;
                border: 2px solid currentColor;
                display: flex;
                flex-direction: column;
                justify-content: center;
                text-align: center;
                padding: 40px;
                position: absolute;
                left: 50%;
            }
            #infographic :not(:nth-child(even)) .circle {
                transform: translateX(-100%);
            }
            #infographic .circle i {
                font-size: 5em;
                margin-bottom: 20px;
                color: currentColor;
            }
            #infographic article {
